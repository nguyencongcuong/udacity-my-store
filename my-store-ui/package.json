{
  "name": "my-store-ui",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "deploy": "npm install -f && npm run build && aws s3 cp --recursive --acl public-read ./dist/my-store-ui s3://my-store-2022/",
